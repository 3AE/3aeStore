<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="store.ae.dao.mall.goods.GoodsTypeDao">
    <!-- 目的： 为dao接口提供sql语句配置 -->

    <select id="queryById" resultType="GoodsType" parameterType="long">
    	select goods_type_id, grade, parent_grade, gmt_create
    	from goods_type
    	where goods_type_id = #{goodsTypeId}
    </select>
</mapper>